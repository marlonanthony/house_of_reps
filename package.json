{
  "name": "houseofreps",
  "version": "1.0.0",
  "description": "Social network for DJs",
  "engines": {
    "node": "8.11.3",
    "yarn": "1.7.0"
  },
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false YARN_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Marlon Decosta",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sendgrid": "^5.2.3",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
